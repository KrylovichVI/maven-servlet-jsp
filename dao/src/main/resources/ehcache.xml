    <cache name="com.krylovichVI.pojo.Book"
           maxEntriesLocalHeap="20"
           eternal="false"
           timeToIdleSeconds="300"
           timeToLiveSeconds="600">
        <persistence strategy="localTempSwap"/>
    </cache>
    <cache name="com.krylovichVI.pojo.Order"
           maxEntriesLocalHeap="20"
           eternal="false"
           timeToIdleSeconds="300"
           timeToLiveSeconds="600">
        <persistence strategy="localTempSwap"/>
    </cache>
